<%= form_for @project do |f|  %>
  <div class="col-md-offset-1">
    <div class="form-group col-md-12">
      <%= f.label :title %>
      <%= f.text_field :title, class: "form-control" %>
    </div>

    <div class="form-group col-md-12">
      <%= f.label :title_slug %>
      <%= f.text_field :title_slug, class: "form-control" %>
    </div>

    <div class="col-md-12">
      <%= f.submit :class=> "btn btn-success" %>
    </div>
  </div>
<% end %>